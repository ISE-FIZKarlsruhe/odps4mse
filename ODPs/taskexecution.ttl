###  http://www.ontologydesignpatterns.org/cp/owl/classification.owl#classifies
classification:classifies rdf:type owl:ObjectProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/classification.owl#isClassifiedBy
classification:isClassifiedBy rdf:type owl:ObjectProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/participation.owl#hasParticipant
participation:hasParticipant rdf:type owl:ObjectProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/taskexecution.owl#executesTask
taskexecution:executesTask rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf classification:isClassifiedBy ;
                           owl:inverseOf taskexecution:isExecutedIn ;
                           rdfs:domain taskexecution:Action ;
                           rdfs:range taskrole:Task ;
                           rdfs:comment "A relation between an action and a task, e.g. 'putting some water in a pot and putting the pot on a fire until the water starts bubbling' executes the task 'boiling'."^^xsd:string ;
                           rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/taskexecution.owl> ;
                           rdfs:label "esegue il task"@it ,
                                      "executes task"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/taskexecution.owl#isExecutedIn
taskexecution:isExecutedIn rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf classification:classifies ;
                           rdfs:domain taskrole:Task ;
                           rdfs:range taskexecution:Action ;
                           rdfs:comment "A relation between an action and a task, e.g. 'putting some water in a pot and putting the pot on a fire until the water starts bubbling' executes the task 'boiling'." ;
                           rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/taskexecution.owl> ;
                           rdfs:label "is executed in"@en ,
                                      "Ã¨ eseguito mediante"@it .


