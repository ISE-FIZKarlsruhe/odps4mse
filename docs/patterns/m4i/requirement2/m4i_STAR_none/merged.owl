    <!-- http://purl.obolibrary.org/obo/BFO_0000054 -->

    <owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/BFO_0000054">
        <owl:inverseOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000055"/>
        <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/BFO_0000017"/>
        <rdfs:comment>Paraphrase of elucidation: a relation between a realizable entity and a process, where there is some material entity that is bearer of the realizable entity and participates in the process, and the realizable entity comes to be realized in the course of the process</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.obolibrary.org/obo/"/>
        <skos:editorialNote xml:lang="en">changed by m4i: added german labels, added skos:prefLabel</skos:editorialNote>
        <skos:prefLabel xml:lang="de">realisiert in</skos:prefLabel>
        <skos:prefLabel xml:lang="en">realized in</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/BFO_0000055 -->

    <owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/BFO_0000055">
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000017"/>
        <rdfs:comment>Paraphrase of elucidation: a relation between a process and a realizable entity, where there is some material entity that is bearer of the realizable entity and participates in the process, and the realizable entity comes to be realized in the course of the process</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.obolibrary.org/obo/"/>
        <skos:editorialNote xml:lang="en">changed by m4i: added german labels, added skos:prefLabel</skos:editorialNote>
        <skos:prefLabel xml:lang="de">realisiert</skos:prefLabel>
        <skos:prefLabel xml:lang="en">realizes</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#hasAdmissibleValue -->

    <owl:ObjectProperty rdf:about="http://w3id.org/nfdi4ing/metadata4ing#hasAdmissibleValue">
        <owl:inverseOf rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#isAdmissibleValueFor"/>
        <rdfs:domain rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#Variable"/>
        <rdfs:range rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#Value"/>
        <rdfs:isDefinedBy rdf:resource="http://w3id.org/nfdi4ing/metadata4ing.owl"/>
        <skos:definition xml:lang="en">(x hasAdmissibleValue y) means that y is a value that can be assigned to the variable x</skos:definition>
        <skos:prefLabel xml:lang="en">hasAdmissibleValue</skos:prefLabel>
        <skos:prefLabel xml:lang="de">hat als zulässigen Wert</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#hasAssignedValue -->

    <owl:ObjectProperty rdf:about="http://w3id.org/nfdi4ing/metadata4ing#hasAssignedValue">
        <owl:inverseOf rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#isValueInAssignment"/>
        <rdfs:domain rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#Assignment"/>
        <rdfs:range rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#Value"/>
        <rdfs:isDefinedBy rdf:resource="http://w3id.org/nfdi4ing/metadata4ing.owl"/>
        <skos:definition xml:lang="en">(a hasAssignedValue y) means that a: x = y is an assignment where the value y is assigned to a variable x</skos:definition>
        <skos:prefLabel xml:lang="en">has assigned value</skos:prefLabel>
        <skos:prefLabel xml:lang="de">hat zugewiesenen Wert</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#hasEmployedTool -->

    <owl:ObjectProperty rdf:about="http://w3id.org/nfdi4ing/metadata4ing#hasEmployedTool">
        <owl:inverseOf rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#isEmployedToolIn"/>
        <rdfs:range rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#Tool"/>
        <rdfs:isDefinedBy rdf:resource="http://w3id.org/nfdi4ing/metadata4ing.owl"/>
        <skos:definition xml:lang="en">Points to a tool that is used within a given processing step</skos:definition>
        <skos:definition xml:lang="de">verweist auf ein Werkzeug, das in einem Arbeitsschritt verwendet wurde</skos:definition>
        <skos:prefLabel xml:lang="en">has employed tool</skos:prefLabel>
        <skos:prefLabel xml:lang="de">hat eingesetztes Werkzeug</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#hasKindOfQuantity -->

    <owl:ObjectProperty rdf:about="http://w3id.org/nfdi4ing/metadata4ing#hasKindOfQuantity">
        <rdfs:domain rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#Variable"/>
        <rdfs:range rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#KindOfQuantity"/>
        <rdfs:isDefinedBy rdf:resource="http://w3id.org/nfdi4ing/metadata4ing.owl"/>
        <skos:definition xml:lang="en">Points to the property characterization (i.e., to the kind of quantity) from a controlled vocabulary</skos:definition>
        <skos:note xml:lang="en">we recommend using a persistent identifier resolving to a persistent URL</skos:note>
        <skos:prefLabel xml:lang="en">has kind of quantity</skos:prefLabel>
        <skos:prefLabel xml:lang="de">hat Größenart</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#hasParameter -->

    <owl:ObjectProperty rdf:about="http://w3id.org/nfdi4ing/metadata4ing#hasParameter">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/nfdi4ing/metadata4ing#Method"/>
                    <rdf:Description rdf:about="http://w3id.org/nfdi4ing/metadata4ing#Tool"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#Variable"/>
        <rdfs:isDefinedBy rdf:resource="http://w3id.org/nfdi4ing/metadata4ing.owl"/>
        <skos:definition xml:lang="en">Points to a parameter of a given method or tool.</skos:definition>
        <skos:prefLabel xml:lang="en">has parameter</skos:prefLabel>
        <skos:prefLabel xml:lang="de">hat Parameter</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#hasUncertaintyDeclaration -->

    <owl:ObjectProperty rdf:about="http://w3id.org/nfdi4ing/metadata4ing#hasUncertaintyDeclaration">
        <rdfs:domain rdf:resource="https://ptb.de/si/Real"/>
        <rdfs:isDefinedBy rdf:resource="http://w3id.org/nfdi4ing/metadata4ing.owl"/>
        <skos:definition xml:lang="en">Associates an si:Real quantity value with its mandatory uncertainty declaration (i.e., with an expanded uncertainty or a coverage interval)</skos:definition>
        <skos:prefLabel xml:lang="en">has uncertainty declaration</skos:prefLabel>
        <skos:prefLabel xml:lang="de">hat Unsicherheitsdeklaration</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#hasUnit -->

    <owl:ObjectProperty rdf:about="http://w3id.org/nfdi4ing/metadata4ing#hasUnit">
        <rdfs:domain rdf:resource="https://ptb.de/si/Real"/>
        <rdfs:isDefinedBy rdf:resource="http://w3id.org/nfdi4ing/metadata4ing.owl"/>
        <skos:closeMatch rdf:resource="http://qudt.org/schema/qudt/unit"/>
        <skos:closeMatch rdf:resource="https://ptb.de/si/hasUnit"/>
        <skos:definition xml:lang="en">Represents the unit of a numerical (real) value</skos:definition>
        <skos:definition xml:lang="de">Repräsentiert die Einheit eines numerischen (reelen) Wertes</skos:definition>
        <skos:note xml:lang="de">Kardinalität dieser Property bei der Beschreibung eines si:Real: 0-1</skos:note>
        <skos:note xml:lang="de">Obligation dieser Property bei der Beschreibung eines si:Real: recommended</skos:note>
        <skos:note xml:lang="en">We recommend using a resolvable PID from a controlled vocabulary, such as ISO 80000, IEC or QUDT</skos:note>
        <skos:note xml:lang="en">cardinality of this property for the description of a si:Real: 0-1</skos:note>
        <skos:note xml:lang="en">obligation of this property for the description of a si:Real: recommended</skos:note>
        <skos:prefLabel xml:lang="en">has unit</skos:prefLabel>
        <skos:prefLabel xml:lang="de">hat Einheit</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#hasVariable -->

    <owl:ObjectProperty rdf:about="http://w3id.org/nfdi4ing/metadata4ing#hasVariable">
        <owl:inverseOf rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#isVariableInAssignment"/>
        <rdfs:domain rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#Assignment"/>
        <rdfs:range rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#Variable"/>
        <rdfs:isDefinedBy rdf:resource="http://w3id.org/nfdi4ing/metadata4ing.owl"/>
        <skos:definition xml:lang="de">( a hasVariable x ) bedeutet, dass a: x = y eine Variablenzuordnung ist, in der der Variablen x ein Wert zugeordnet wird.</skos:definition>
        <skos:definition xml:lang="en">( a hasVariable x ) means that a: x = y is an assignment where the value y is assigned to a variable x</skos:definition>
        <skos:prefLabel xml:lang="en">has variable</skos:prefLabel>
        <skos:prefLabel xml:lang="de">hat Variable</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#implementedByTool -->

    <owl:ObjectProperty rdf:about="http://w3id.org/nfdi4ing/metadata4ing#implementedByTool">
        <owl:inverseOf rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#implementsMethod"/>
        <rdfs:domain rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#Method"/>
        <rdfs:range rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#Tool"/>
        <rdfs:isDefinedBy rdf:resource="http://w3id.org/nfdi4ing/metadata4ing.owl"/>
        <skos:definition xml:lang="en">Points to a tool that implements a given method</skos:definition>
        <skos:prefLabel xml:lang="en">implemented by</skos:prefLabel>
        <skos:prefLabel xml:lang="de">wird implementiert durch</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#implementsMethod -->

    <owl:ObjectProperty rdf:about="http://w3id.org/nfdi4ing/metadata4ing#implementsMethod">
        <rdfs:domain rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#Tool"/>
        <rdfs:range rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#Method"/>
        <rdfs:isDefinedBy rdf:resource="http://w3id.org/nfdi4ing/metadata4ing.owl"/>
        <skos:definition xml:lang="en">Points to a method that is implemented by a given tool</skos:definition>
        <skos:prefLabel xml:lang="de">implementiert</skos:prefLabel>
        <skos:prefLabel xml:lang="en">implements</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#isEmployedToolIn -->

    <owl:ObjectProperty rdf:about="http://w3id.org/nfdi4ing/metadata4ing#isEmployedToolIn">
        <rdfs:domain rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#Tool"/>
        <rdfs:isDefinedBy rdf:resource="http://w3id.org/nfdi4ing/metadata4ing.owl"/>
        <skos:definition xml:lang="en">Points to a processing step the tool has been used in</skos:definition>
        <skos:definition xml:lang="de">verweist auf einen Arbeitsschritt, in dem das Werkzeug eingesetzt wurde</skos:definition>
        <skos:prefLabel xml:lang="en">is employed tool</skos:prefLabel>
        <skos:prefLabel xml:lang="de">ist eingesetztes Werkzeug</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#realizesMethod -->

    <owl:ObjectProperty rdf:about="http://w3id.org/nfdi4ing/metadata4ing#realizesMethod">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000055"/>
        <rdfs:range rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#Method"/>
        <rdfs:isDefinedBy rdf:resource="http://w3id.org/nfdi4ing/metadata4ing.owl"/>
        <skos:definition xml:lang="en">Points to a method that is used within a given processing step</skos:definition>
        <skos:prefLabel xml:lang="de">realisiert Methode</skos:prefLabel>
        <skos:prefLabel xml:lang="en">realizes method</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.molmod.info/semantics/pims-ii.ttl#isAdmissibleValueFor -->

    <owl:ObjectProperty rdf:about="http://www.molmod.info/semantics/pims-ii.ttl#isAdmissibleValueFor">
        <rdfs:domain rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#Value"/>
        <rdfs:range rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#Variable"/>
        <rdfs:isDefinedBy rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl.owl"/>
        <skos:definition xml:lang="en">(x isAdmissibleValueFor y) means that x is a value that can be assigned to the variable y; subsumption under msIPIRIR is explained as follows: There is a sign z for the variable y (where z is the range of admissible values of y), for which in turn there is another sign z&apos; (where z&apos; lists members of z, explicitly including x) of which x is a part</skos:definition>
        <skos:editorialNote>changed by m4i: added skos:prefLabel, adjusted german labels, harmonized spelling to OBO naming conventions (lower case, space separated)</skos:editorialNote>
        <skos:prefLabel xml:lang="en">is admissible value for</skos:prefLabel>
        <skos:prefLabel xml:lang="de">ist zulässiger Wert für</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.molmod.info/semantics/pims-ii.ttl#isAssignmentFor -->

    <owl:ObjectProperty rdf:about="http://www.molmod.info/semantics/pims-ii.ttl#isAssignmentFor">
        <rdfs:domain rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#Assignment"/>
        <rdfs:isDefinedBy rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl.owl"/>
        <skos:broader rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#isRepresentamenFor"/>
        <skos:definition xml:lang="en">(o isAssignment for d) means that d: x = y is an assignment that refers to o; e.g., x may be temperature, y may be a temperature value, and o may be an object of which the temperature has been measured or some other context to which the assignment refers</skos:definition>
        <skos:editorialNote>changed by m4i: added skos:prefLabel, adjusted german labels, harmonized spelling to OBO naming conventions (lower case, space separated)</skos:editorialNote>
        <skos:prefLabel xml:lang="en">is assignment for</skos:prefLabel>
        <skos:prefLabel xml:lang="de">ist Zuweisung in Bezug auf</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.molmod.info/semantics/pims-ii.ttl#isValueInAssignment -->

    <owl:ObjectProperty rdf:about="http://www.molmod.info/semantics/pims-ii.ttl#isValueInAssignment">
        <rdfs:domain rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#Value"/>
        <rdfs:range rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#Assignment"/>
        <rdfs:isDefinedBy rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl.owl"/>
        <skos:broader rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#isSemioticallyConstitutiveOf"/>
        <skos:definition xml:lang="en">(y isValueInAssignment a) means that a: x = y is an assignment where the value y is assigned to a variable x</skos:definition>
        <skos:editorialNote>changed by m4i: added skos:prefLabel, adjusted german labels, harmonized spelling to OBO naming conventions (lower case, space separated)</skos:editorialNote>
        <skos:prefLabel xml:lang="en">is value in assignment</skos:prefLabel>
        <skos:prefLabel xml:lang="de">ist Wert in Zuweisung</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.molmod.info/semantics/pims-ii.ttl#isVariableInAssignment -->

    <owl:ObjectProperty rdf:about="http://www.molmod.info/semantics/pims-ii.ttl#isVariableInAssignment">
        <rdfs:domain rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#Variable"/>
        <rdfs:range rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#Assignment"/>
        <rdfs:isDefinedBy rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl.owl"/>
        <skos:broader rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#isSemioticallyConstitutiveOf"/>
        <skos:definition xml:lang="en">(x isVariableInAssignment a) means that a: x = y is an assignment where a value y is assigned to the variable x</skos:definition>
        <skos:editorialNote>changed by m4i: added skos:prefLabel, adjusted german labels, harmonized spelling to OBO naming conventions (lower case, space separated)</skos:editorialNote>
        <skos:prefLabel xml:lang="en">is variable in assignment</skos:prefLabel>
        <skos:prefLabel xml:lang="de">ist Variable in Zuweisung</skos:prefLabel>
    </owl:ObjectProperty>
    


