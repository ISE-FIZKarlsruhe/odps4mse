    <!-- http://w3id.org/nfdi4ing/metadata4ing#hasNumericalValue -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/nfdi4ing/metadata4ing#hasNumericalValue">
        <rdfs:domain rdf:resource="https://ptb.de/si/Real"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:isDefinedBy rdf:resource="http://w3id.org/nfdi4ing/metadata4ing.owl"/>
        <skos:broader rdf:resource="http://qudt.org/schema/qudt/value"/>
        <skos:broader rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#isLiterally"/>
        <skos:broader rdf:resource="https://ptb.de/si/hasValue"/>
        <skos:closeMatch rdf:resource="https://ptb.de/si/hasNumericalValue"/>
        <skos:definition xml:lang="en">Represents the numerical value of a real</skos:definition>
        <skos:prefLabel xml:lang="en">has numerical value</skos:prefLabel>
        <skos:prefLabel xml:lang="de">hat Zahlenwert</skos:prefLabel>
    </owl:DatatypeProperty>
    


