@prefix : <http://OntoCAPE/chemical_process_system/CPS_function/process.owl#> .
@prefix fc: <http://meta_model/fundamental_concepts/fundamental_concepts.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix system: <http://OntoCAPE/upper_level/system.owl#> .
@prefix der_dim: <http://OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#> .
@prefix process: <http://OntoCAPE/chemical_process_system/CPS_function/process.owl#> .
@prefix material: <http://OntoCAPE/material/material.owl#> .
@prefix phys_dim: <http://OntoCAPE/supporting_concepts/physical_dimension/physical_dimension.owl#> .
@prefix phase_system: <http://OntoCAPE/material/phase_system/phase_system.owl#> .
@prefix network_system: <http://OntoCAPE/upper_level/network_system.owl#> .
@base <http://OntoCAPE/chemical_process_system/CPS_function/process.owl#> .

<http://OntoCAPE/chemical_process_system/CPS_function/process.owl> rdf:type owl:Ontology ;
