<?xml version="1.0"?>
<rdf:RDF xmlns="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#"
     xml:base="http://OntoCAPE/chemical_process_system/CPS_function/process.owl"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://OntoCAPE/chemical_process_system/CPS_function/process.owl"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2000/01/rdf-schema#comment -->

    <rdf:Description rdf:about="http://www.w3.org/2000/01/rdf-schema#comment">
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2000/01/rdf-schema.owl"/>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://OntoCAPE/upper_level/network_system.owl#enters -->

    <owl:ObjectProperty rdf:about="http://OntoCAPE/upper_level/network_system.owl#enters">
        <rdfs:subPropertyOf rdf:resource="http://OntoCAPE/upper_level/network_system.owl#isPredecessorOf"/>
        <owl:inverseOf rdf:resource="http://OntoCAPE/upper_level/network_system.owl#hasInput"/>
        <rdfs:domain rdf:resource="http://OntoCAPE/upper_level/network_system.owl#DirectedConnection"/>
        <rdfs:range rdf:resource="http://OntoCAPE/upper_level/network_system.owl#Device"/>
        <rdfs:comment>The relation Enters interconnects an ingoing DirectedConnection  to its target Device.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontocape/upper_level/network_system.owl"/>
    </owl:ObjectProperty>
    


    <!-- http://OntoCAPE/upper_level/network_system.owl#hasInput -->

    <owl:ObjectProperty rdf:about="http://OntoCAPE/upper_level/network_system.owl#hasInput">
        <rdfs:subPropertyOf rdf:resource="http://OntoCAPE/upper_level/network_system.owl#isSuccessorOf"/>
        <rdfs:domain rdf:resource="http://OntoCAPE/upper_level/network_system.owl#Device"/>
        <rdfs:range rdf:resource="http://OntoCAPE/upper_level/network_system.owl#DirectedConnection"/>
        <rdfs:comment>The relation hasInput connects a Device to an incoming DirectedConnection.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontocape/upper_level/network_system.owl"/>
    </owl:ObjectProperty>
    


    <!-- http://OntoCAPE/upper_level/network_system.owl#hasOutput -->

    <owl:ObjectProperty rdf:about="http://OntoCAPE/upper_level/network_system.owl#hasOutput">
        <rdfs:subPropertyOf rdf:resource="http://OntoCAPE/upper_level/network_system.owl#isPredecessorOf"/>
        <owl:inverseOf rdf:resource="http://OntoCAPE/upper_level/network_system.owl#leaves"/>
        <rdfs:domain rdf:resource="http://OntoCAPE/upper_level/network_system.owl#Device"/>
        <rdfs:range rdf:resource="http://OntoCAPE/upper_level/network_system.owl#DirectedConnection"/>
        <rdfs:comment>The relation hasOutput connects a Device to an outgoing DirectedConnection.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontocape/upper_level/network_system.owl"/>
    </owl:ObjectProperty>
    


    <!-- http://OntoCAPE/upper_level/network_system.owl#isPredecessorOf -->

    <owl:ObjectProperty rdf:about="http://OntoCAPE/upper_level/network_system.owl#isPredecessorOf">
        <rdfs:subPropertyOf rdf:resource="http://OntoCAPE/upper_level/system.owl#isConnectedTo"/>
        <owl:inverseOf rdf:resource="http://OntoCAPE/upper_level/network_system.owl#isSuccessorOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://OntoCAPE/upper_level/network_system.owl#Device"/>
                    <rdf:Description rdf:about="http://OntoCAPE/upper_level/network_system.owl#DirectedConnection"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://OntoCAPE/upper_level/network_system.owl#Device"/>
                    <rdf:Description rdf:about="http://OntoCAPE/upper_level/network_system.owl#DirectedConnection"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>The relation isPredecessorOf identifies all Devices and DirectedConnections that are predecessors of the considered one.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontocape/upper_level/network_system.owl"/>
    </owl:ObjectProperty>
    


    <!-- http://OntoCAPE/upper_level/network_system.owl#isSuccessorOf -->

    <owl:ObjectProperty rdf:about="http://OntoCAPE/upper_level/network_system.owl#isSuccessorOf">
        <rdfs:subPropertyOf rdf:resource="http://OntoCAPE/upper_level/system.owl#isConnectedTo"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://OntoCAPE/upper_level/network_system.owl#Device"/>
                    <rdf:Description rdf:about="http://OntoCAPE/upper_level/network_system.owl#DirectedConnection"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://OntoCAPE/upper_level/network_system.owl#Device"/>
                    <rdf:Description rdf:about="http://OntoCAPE/upper_level/network_system.owl#DirectedConnection"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>The relation isSuccessorOf identifies all Devices and DirectedConnections that are successors of the considered one.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontocape/upper_level/network_system.owl"/>
    </owl:ObjectProperty>
    


    <!-- http://OntoCAPE/upper_level/network_system.owl#leaves -->

    <owl:ObjectProperty rdf:about="http://OntoCAPE/upper_level/network_system.owl#leaves">
        <rdfs:subPropertyOf rdf:resource="http://OntoCAPE/upper_level/network_system.owl#isSuccessorOf"/>
        <rdfs:domain rdf:resource="http://OntoCAPE/upper_level/network_system.owl#DirectedConnection"/>
        <rdfs:range rdf:resource="http://OntoCAPE/upper_level/network_system.owl#Device"/>
        <rdfs:comment>The relation Leaves connects an outgoing DirectedConnection to its source Device.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontocape/upper_level/network_system.owl"/>
    </owl:ObjectProperty>
    


    <!-- http://OntoCAPE/upper_level/system.owl#hasDirectSubsystem -->

    <owl:ObjectProperty rdf:about="http://OntoCAPE/upper_level/system.owl#hasDirectSubsystem">
        <rdfs:subPropertyOf rdf:resource="http://OntoCAPE/upper_level/system.owl#hasSubsystem"/>
        <rdfs:subPropertyOf rdf:resource="http://OntoCAPE/upper_level/system.owl#isDirectlyRelatedTo"/>
        <owl:inverseOf rdf:resource="http://OntoCAPE/upper_level/system.owl#isDirectSubsystemOf"/>
        <rdfs:comment>The relation hasDirectSubsystem refers from a Supersystem to its direct Subsystem.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontocape/upper_level/system.owl"/>
    </owl:ObjectProperty>
    


    <!-- http://OntoCAPE/upper_level/system.owl#hasSubsystem -->

    <owl:ObjectProperty rdf:about="http://OntoCAPE/upper_level/system.owl#hasSubsystem">
        <rdfs:subPropertyOf rdf:resource="http://OntoCAPE/upper_level/system.owl#isRelatedTo"/>
        <owl:inverseOf rdf:resource="http://OntoCAPE/upper_level/system.owl#isSubsystemOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>The relation hasSubsystem denotes the relation between a Supersystem and its Subsystem.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontocape/upper_level/system.owl"/>
    </owl:ObjectProperty>
    


    <!-- http://OntoCAPE/upper_level/system.owl#isComposedOfSubsystem -->

    <owl:ObjectProperty rdf:about="http://OntoCAPE/upper_level/system.owl#isComposedOfSubsystem">
        <rdfs:subPropertyOf rdf:resource="http://OntoCAPE/upper_level/system.owl#hasDirectSubsystem"/>
        <owl:inverseOf rdf:resource="http://OntoCAPE/upper_level/system.owl#isExclusivelySubsystemOf"/>
        <rdfs:comment>The relation isComposedOf indicates the non-sharable, direct Subsystem of a Supersystem.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontocape/upper_level/system.owl"/>
    </owl:ObjectProperty>
    


    <!-- http://OntoCAPE/upper_level/system.owl#isConnectedTo -->

    <owl:ObjectProperty rdf:about="http://OntoCAPE/upper_level/system.owl#isConnectedTo">
        <rdfs:subPropertyOf rdf:resource="http://OntoCAPE/upper_level/system.owl#isRelatedTo"/>
        <owl:inverseOf rdf:resource="http://OntoCAPE/upper_level/system.owl#isConnectedTo"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>The relation isConnectedTo represents topological connectivity between System.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontocape/upper_level/system.owl"/>
    </owl:ObjectProperty>
    


    <!-- http://OntoCAPE/upper_level/system.owl#isDirectSubsystemOf -->

    <owl:ObjectProperty rdf:about="http://OntoCAPE/upper_level/system.owl#isDirectSubsystemOf">
        <rdfs:subPropertyOf rdf:resource="http://OntoCAPE/upper_level/system.owl#isDirectlyRelatedTo"/>
        <rdfs:subPropertyOf rdf:resource="http://OntoCAPE/upper_level/system.owl#isSubsystemOf"/>
        <rdfs:comment>The relation isDirectlySubsystemOf links a Subsystem to its direct Supersystem.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontocape/upper_level/system.owl"/>
    </owl:ObjectProperty>
    


    <!-- http://OntoCAPE/upper_level/system.owl#isDirectlyRelatedTo -->

    <owl:ObjectProperty rdf:about="http://OntoCAPE/upper_level/system.owl#isDirectlyRelatedTo">
        <rdfs:subPropertyOf rdf:resource="http://OntoCAPE/upper_level/system.owl#isRelatedTo"/>
        <owl:inverseOf rdf:resource="http://OntoCAPE/upper_level/system.owl#isDirectlyRelatedTo"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:comment>The relation isDirectlyRelatedTo subsumes all kinds of direct inter-system relations.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontocape/upper_level/system.owl"/>
    </owl:ObjectProperty>
    


    <!-- http://OntoCAPE/upper_level/system.owl#isExclusivelySubsystemOf -->

    <owl:ObjectProperty rdf:about="http://OntoCAPE/upper_level/system.owl#isExclusivelySubsystemOf">
        <rdfs:subPropertyOf rdf:resource="http://OntoCAPE/upper_level/system.owl#isDirectSubsystemOf"/>
        <rdfs:comment>The relation isExclusivelySubsystemOf links a non-sharable Subsystem to its direct Supersystem.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontocape/upper_level/system.owl"/>
    </owl:ObjectProperty>
    


    <!-- http://OntoCAPE/upper_level/system.owl#isRelatedTo -->

    <owl:ObjectProperty rdf:about="http://OntoCAPE/upper_level/system.owl#isRelatedTo">
        <owl:inverseOf rdf:resource="http://OntoCAPE/upper_level/system.owl#isRelatedTo"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>The relation isRelatedTo subsumes all kinds of inter-system relations.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontocape/upper_level/system.owl"/>
    </owl:ObjectProperty>
    


    <!-- http://OntoCAPE/upper_level/system.owl#isSubsystemOf -->

    <owl:ObjectProperty rdf:about="http://OntoCAPE/upper_level/system.owl#isSubsystemOf">
        <rdfs:subPropertyOf rdf:resource="http://OntoCAPE/upper_level/system.owl#isRelatedTo"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>The relation isSubsystemOf refers from a Subsystem to its Supersystem.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontocape/upper_level/system.owl"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://OntoCAPE/chemical_process_system/CPS_function/process.owl#AggregatedProcessStep -->

    <owl:Class rdf:about="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#AggregatedProcessStep">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessStep"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://OntoCAPE/upper_level/system.owl#hasDirectSubsystem"/>
                        <owl:someValuesFrom rdf:resource="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessStep"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>An AggregatedProcessStep is a ProcessStep which consists of a number of ProcessSteps in a certain sequence.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontocape/chemical_process_system/cps_function/process.owl"/>
    </owl:Class>
    


    <!-- http://OntoCAPE/chemical_process_system/CPS_function/process.owl#OutputProduct -->

    <owl:Class rdf:about="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#OutputProduct">
        <rdfs:subClassOf rdf:resource="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessState"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://OntoCAPE/upper_level/network_system.owl#leaves"/>
                <owl:someValuesFrom rdf:resource="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessStep"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://OntoCAPE/upper_level/network_system.owl#enters"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An OutputProduct is a Material that is output by a ProcessStep.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontocape/chemical_process_system/cps_function/process.owl"/>
    </owl:Class>
    


    <!-- http://OntoCAPE/chemical_process_system/CPS_function/process.owl#Process -->

    <owl:Class rdf:about="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#Process">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#AggregatedProcessStep"/>
                    <owl:Class>
                        <owl:complementOf>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://OntoCAPE/upper_level/system.owl#isSubsystemOf"/>
                                <owl:someValuesFrom rdf:resource="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessStep"/>
                            </owl:Restriction>
                        </owl:complementOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>An AggregatedProcessStep representing the entire function of the Process considered</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontocape/chemical_process_system/cps_function/process.owl"/>
    </owl:Class>
    


    <!-- http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessState -->

    <owl:Class rdf:about="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessState">
        <rdfs:subClassOf rdf:resource="http://OntoCAPE/upper_level/network_system.owl#DirectedConnection"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://OntoCAPE/upper_level/network_system.owl#enters"/>
                <owl:allValuesFrom rdf:resource="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessStep"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://OntoCAPE/upper_level/network_system.owl#leaves"/>
                <owl:allValuesFrom rdf:resource="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessStep"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://OntoCAPE/upper_level/system.owl#hasDirectSubsystem"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessState"/>
                            <rdf:Description rdf:about="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessStep"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://OntoCAPE/upper_level/system.owl#isDirectSubsystemOf"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessState"/>
                            <rdf:Description rdf:about="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessStep"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A ProcessState represents the collectivity of properties of a CertainMaterialAmount  which is produced in the associating process step.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontocape/chemical_process_system/cps_function/process.owl"/>
    </owl:Class>
    


    <!-- http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessStep -->

    <owl:Class rdf:about="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessStep">
        <rdfs:subClassOf rdf:resource="http://OntoCAPE/upper_level/network_system.owl#Device"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://OntoCAPE/upper_level/network_system.owl#hasInput"/>
                <owl:someValuesFrom rdf:resource="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessState"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://OntoCAPE/upper_level/network_system.owl#hasOutput"/>
                <owl:someValuesFrom rdf:resource="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessState"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://OntoCAPE/upper_level/network_system.owl#hasInput"/>
                <owl:allValuesFrom rdf:resource="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessState"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://OntoCAPE/upper_level/network_system.owl#hasOutput"/>
                <owl:allValuesFrom rdf:resource="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessState"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://OntoCAPE/upper_level/system.owl#hasDirectSubsystem"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessState"/>
                            <rdf:Description rdf:about="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessStep"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://OntoCAPE/upper_level/system.owl#isDirectSubsystemOf"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessState"/>
                            <rdf:Description rdf:about="http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessStep"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A ProcessStep is a certain material processing procedure.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontocape/chemical_process_system/cps_function/process.owl"/>
    </owl:Class>
    


    <!-- http://OntoCAPE/upper_level/network_system.owl#Device -->

    <owl:Class rdf:about="http://OntoCAPE/upper_level/network_system.owl#Device">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://OntoCAPE/upper_level/network_system.owl#hasInput"/>
                <owl:allValuesFrom rdf:resource="http://OntoCAPE/upper_level/network_system.owl#DirectedConnection"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://OntoCAPE/upper_level/network_system.owl#hasOutput"/>
                <owl:allValuesFrom rdf:resource="http://OntoCAPE/upper_level/network_system.owl#DirectedConnection"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Devices are the crucial elements of a NetworkSystem, holding the major functionality.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontocape/upper_level/network_system.owl"/>
    </owl:Class>
    


    <!-- http://OntoCAPE/upper_level/network_system.owl#DirectedConnection -->

    <owl:Class rdf:about="http://OntoCAPE/upper_level/network_system.owl#DirectedConnection">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://OntoCAPE/upper_level/network_system.owl#enters"/>
                <owl:allValuesFrom rdf:resource="http://OntoCAPE/upper_level/network_system.owl#Device"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://OntoCAPE/upper_level/network_system.owl#leaves"/>
                <owl:allValuesFrom rdf:resource="http://OntoCAPE/upper_level/network_system.owl#Device"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://OntoCAPE/upper_level/network_system.owl#enters"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://OntoCAPE/upper_level/network_system.owl#leaves"/>
