@prefix : <http://OntoCAPE/chemical_process_system/CPS_function/process.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://OntoCAPE/chemical_process_system/CPS_function/process.owl> .

<http://OntoCAPE/chemical_process_system/CPS_function/process.owl> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2000/01/rdf-schema#comment
rdfs:comment rdfs:isDefinedBy <http://www.w3.org/2000/01/rdf-schema.owl> .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2000/01/rdf-schema#Literal
rdfs:Literal rdfs:isDefinedBy <http://www.w3.org/2000/01/rdf-schema.owl> .


###  http://www.w3.org/2001/XMLSchema#integer
xsd:integer rdfs:isDefinedBy <http://www.w3.org/2001/xmlschema.owl> .


###  http://www.w3.org/2001/XMLSchema#string
xsd:string rdfs:isDefinedBy <http://www.w3.org/2001/xmlschema.owl> .


#################################################################
#    Object Properties
#################################################################

###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#hasChemicalReactionNetwork
:hasChemicalReactionNetwork rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#isDirectlyRelatedTo> ;
                            rdfs:comment "The relation hasChemicalRectionNetwork indicates that a reaction as considered on a rather macroscopic perspective of an early design stage may be the result of an sequence of reactions occurring on the bases of several phases." ;
                            rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#hasOperationMode
:hasOperationMode rdf:type owl:ObjectProperty ;
                  rdfs:comment "The relation hasOperationMode indicates by which operation modes a particular process state is achieved." ;
                  rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#hasPressureDifference
:hasPressureDifference rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#hasProperty> ;
                       rdfs:comment "The relation hasPressureDifference indicates the intended difference between two pressure state." ;
                       rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#hasVapourRatio
:hasVapourRatio rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#hasProperty> ;
                rdfs:comment "The relation hasVaporRatio indicates the liquid-vapor ratio of a particular mixture at a certain state within a vessel." ;
                rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/upper_level/network_system.owl#enters
<http://OntoCAPE/upper_level/network_system.owl#enters> rdf:type owl:ObjectProperty ;
                                                        rdfs:subPropertyOf <http://OntoCAPE/upper_level/network_system.owl#isPredecessorOf> ;
                                                        owl:inverseOf <http://OntoCAPE/upper_level/network_system.owl#hasInput> ;
                                                        rdfs:comment "The relation Enters interconnects an ingoing DirectedConnection  to its target Device." ;
                                                        rdfs:isDefinedBy <http://ontocape/upper_level/network_system.owl> .


###  http://OntoCAPE/upper_level/network_system.owl#hasInput
<http://OntoCAPE/upper_level/network_system.owl#hasInput> rdf:type owl:ObjectProperty ;
                                                          rdfs:subPropertyOf <http://OntoCAPE/upper_level/network_system.owl#isSuccessorOf> ;
                                                          rdfs:comment "The relation hasInput connects a Device to an incoming DirectedConnection." ;
                                                          rdfs:isDefinedBy <http://ontocape/upper_level/network_system.owl> .


###  http://OntoCAPE/upper_level/network_system.owl#hasOutput
<http://OntoCAPE/upper_level/network_system.owl#hasOutput> rdf:type owl:ObjectProperty ;
                                                           rdfs:subPropertyOf <http://OntoCAPE/upper_level/network_system.owl#isPredecessorOf> ;
                                                           owl:inverseOf <http://OntoCAPE/upper_level/network_system.owl#leaves> ;
                                                           rdfs:comment "The relation hasOutput connects a Device to an outgoing DirectedConnection." ;
                                                           rdfs:isDefinedBy <http://ontocape/upper_level/network_system.owl> .


###  http://OntoCAPE/upper_level/network_system.owl#isPredecessorOf
<http://OntoCAPE/upper_level/network_system.owl#isPredecessorOf> rdf:type owl:ObjectProperty ;
                                                                 rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#isConnectedTo> ;
                                                                 owl:inverseOf <http://OntoCAPE/upper_level/network_system.owl#isSuccessorOf> ;
                                                                 rdf:type owl:TransitiveProperty ;
                                                                 rdfs:comment "The relation isPredecessorOf identifies all Devices and DirectedConnections that are predecessors of the considered one." ;
                                                                 rdfs:isDefinedBy <http://ontocape/upper_level/network_system.owl> .


###  http://OntoCAPE/upper_level/network_system.owl#isSuccessorOf
<http://OntoCAPE/upper_level/network_system.owl#isSuccessorOf> rdf:type owl:ObjectProperty ;
                                                               rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#isConnectedTo> ;
                                                               rdf:type owl:TransitiveProperty ;
                                                               rdfs:comment "The relation isSuccessorOf identifies all Devices and DirectedConnections that are successors of the considered one." ;
                                                               rdfs:isDefinedBy <http://ontocape/upper_level/network_system.owl> .


###  http://OntoCAPE/upper_level/network_system.owl#leaves
<http://OntoCAPE/upper_level/network_system.owl#leaves> rdf:type owl:ObjectProperty ;
                                                        rdfs:subPropertyOf <http://OntoCAPE/upper_level/network_system.owl#isSuccessorOf> ;
                                                        rdfs:comment "The relation Leaves connects an outgoing DirectedConnection to its source Device." ;
                                                        rdfs:isDefinedBy <http://ontocape/upper_level/network_system.owl> .


###  http://OntoCAPE/upper_level/network_system.owl#sameAs
<http://OntoCAPE/upper_level/network_system.owl#sameAs> rdf:type owl:ObjectProperty ;
                                                        rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#isDirectlyRelatedTo> ;
                                                        owl:inverseOf <http://OntoCAPE/upper_level/network_system.owl#sameAs> ;
                                                        rdf:type owl:SymmetricProperty ;
                                                        rdfs:comment "The relation denotes a correspondence between a Connection and its placeholder in a composition hierarchy" ;
                                                        rdfs:isDefinedBy <http://ontocape/upper_level/network_system.owl> .


###  http://OntoCAPE/upper_level/system.owl#comprises
<http://OntoCAPE/upper_level/system.owl#comprises> rdf:type owl:ObjectProperty ,
                                                            owl:TransitiveProperty ;
                                                   rdfs:range <http://OntoCAPE/upper_level/system.owl#Property> ;
                                                   rdfs:comment "The relation Comprises indicates the members of a PropertySet." ;
                                                   rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#comprisesDirectly
<http://OntoCAPE/upper_level/system.owl#comprisesDirectly> rdf:type owl:ObjectProperty ;
                                                           rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#comprises> ;
                                                           rdfs:comment "The relation ComprisesDirectly indicates the direct members of a PropertySet." ;
                                                           rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#contains
<http://OntoCAPE/upper_level/system.owl#contains> rdf:type owl:ObjectProperty ;
                                                  rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#isRelatedTo> ;
                                                  rdf:type owl:TransitiveProperty ;
                                                  rdfs:comment """The Contains relation constitutes an alternative to the hasSubsystem relation. It should be used instead of hasSubsystem 
-   if the hasSubsystem relation causes performance problems, or
-   if only one side of the aggregation relation is of interest, namely the indication of the constituting elements of a supersystem""" ;
                                                  rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#containsDirectly
<http://OntoCAPE/upper_level/system.owl#containsDirectly> rdf:type owl:ObjectProperty ;
                                                          rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#contains> ,
                                                                             <http://OntoCAPE/upper_level/system.owl#isDirectlyRelatedTo> ;
                                                          rdfs:comment """The relation containsDirectly is an alternative to the hasDirectSubsystem relation. It should be used instead of hasDirectSubsystem 
-   if the hasDirectSubsystem relation causes performance problems, or
-   if only one side of the aggregation relation is of interest, namely the indication of the direct constituents Supersystem.""" ;
                                                          rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#hasAspectSystem
<http://OntoCAPE/upper_level/system.owl#hasAspectSystem> rdf:type owl:ObjectProperty ;
                                                         rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#isComposedOfSubsystem> ;
                                                         owl:inverseOf <http://OntoCAPE/upper_level/system.owl#representsAspectOf> ;
                                                         rdfs:comment "The relation hasAspectSystem designates the AspectSystems of a System." ;
                                                         rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#hasCharacteristic
<http://OntoCAPE/upper_level/system.owl#hasCharacteristic> rdf:type owl:ObjectProperty ;
                                                           rdfs:domain <http://OntoCAPE/upper_level/system.owl#System> ;
                                                           rdfs:range [ rdf:type owl:Class ;
                                                                        owl:unionOf ( <http://OntoCAPE/upper_level/system.owl#QuantitativeValue>
                                                                                      <http://OntoCAPE/upper_level/system.owl#ValueEnumeration>
                                                                                    )
                                                                      ] ;
                                                           rdfs:comment "The hasCharacteristic relation constitutes a shorthand notation for the specification of a ConstantProperty and its Value." ;
                                                           rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#hasDimension
<http://OntoCAPE/upper_level/system.owl#hasDimension> rdf:type owl:ObjectProperty ,
                                                               owl:FunctionalProperty ;
                                                      rdfs:domain [ rdf:type owl:Class ;
                                                                    owl:unionOf ( <http://OntoCAPE/upper_level/system.owl#Property>
                                                                                  <http://OntoCAPE/upper_level/system.owl#UnitOfMeasure>
                                                                                )
                                                                  ] ;
                                                      rdfs:range <http://OntoCAPE/upper_level/system.owl#PhysicalDimension> ;
                                                      rdfs:comment "The relation hasDimension specifies the PhysicalDimension of a PhysicalQuantity or a UnitOfMeasure." ;
                                                      rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#hasDirectSubsystem
<http://OntoCAPE/upper_level/system.owl#hasDirectSubsystem> rdf:type owl:ObjectProperty ;
                                                            rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#hasSubsystem> ,
                                                                               <http://OntoCAPE/upper_level/system.owl#isDirectlyRelatedTo> ;
                                                            owl:inverseOf <http://OntoCAPE/upper_level/system.owl#isDirectSubsystemOf> ;
                                                            rdfs:comment "The relation hasDirectSubsystem refers from a Supersystem to its direct Subsystem." ;
                                                            rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#hasProperty
<http://OntoCAPE/upper_level/system.owl#hasProperty> rdf:type owl:ObjectProperty ;
                                                     owl:inverseOf <http://OntoCAPE/upper_level/system.owl#isPropertyOf> ;
                                                     rdf:type owl:InverseFunctionalProperty ;
                                                     rdfs:domain <http://OntoCAPE/upper_level/system.owl#System> ;
                                                     rdfs:range <http://OntoCAPE/upper_level/system.owl#Property> ;
                                                     rdfs:comment "The relation hasProperty indicates the Properties of a System." ;
                                                     rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#hasSubsystem
<http://OntoCAPE/upper_level/system.owl#hasSubsystem> rdf:type owl:ObjectProperty ;
                                                      rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#isRelatedTo> ;
                                                      owl:inverseOf <http://OntoCAPE/upper_level/system.owl#isSubsystemOf> ;
                                                      rdf:type owl:TransitiveProperty ;
                                                      rdfs:comment "The relation hasSubsystem denotes the relation between a Supersystem and its Subsystem." ;
                                                      rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#hasUnitOfMeasure
<http://OntoCAPE/upper_level/system.owl#hasUnitOfMeasure> rdf:type owl:ObjectProperty ,
                                                                   owl:FunctionalProperty ;
                                                          rdfs:domain <http://OntoCAPE/upper_level/system.owl#QuantitativeValue> ;
                                                          rdfs:range <http://OntoCAPE/upper_level/system.owl#UnitOfMeasure> ;
                                                          rdfs:comment "The relation hasUnitOfMeasure establishes the UnitOfMeasure of a ScalarValue." ;
                                                          rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#hasValue
<http://OntoCAPE/upper_level/system.owl#hasValue> rdf:type owl:ObjectProperty ;
                                                  owl:inverseOf <http://OntoCAPE/upper_level/system.owl#isValueOf> ;
                                                  rdf:type owl:InverseFunctionalProperty ;
                                                  rdfs:domain <http://OntoCAPE/upper_level/system.owl#Property> ;
                                                  rdfs:range <http://OntoCAPE/upper_level/system.owl#Value> ;
                                                  rdfs:comment "The hasValue relation designates the Values of a Property." ;
                                                  rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#isBackdropOf
<http://OntoCAPE/upper_level/system.owl#isBackdropOf> rdf:type owl:ObjectProperty ;
                                                      owl:inverseOf <http://OntoCAPE/upper_level/system.owl#isObservedAgainstBackdrop> ;
                                                      rdfs:domain <http://OntoCAPE/upper_level/system.owl#Value> ;
                                                      rdfs:range <http://OntoCAPE/upper_level/system.owl#Value> ;
                                                      rdfs:comment "The isBackdropOf relation states that the Value serves as a backdrop for the observation of some other Value." ;
                                                      rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#isComposedOfSubsystem
<http://OntoCAPE/upper_level/system.owl#isComposedOfSubsystem> rdf:type owl:ObjectProperty ;
                                                               rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#hasDirectSubsystem> ;
                                                               owl:inverseOf <http://OntoCAPE/upper_level/system.owl#isExclusivelySubsystemOf> ;
                                                               rdfs:comment "The relation isComposedOf indicates the non-sharable, direct Subsystem of a Supersystem." ;
                                                               rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#isConnectedTo
<http://OntoCAPE/upper_level/system.owl#isConnectedTo> rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#isRelatedTo> ;
                                                       owl:inverseOf <http://OntoCAPE/upper_level/system.owl#isConnectedTo> ;
                                                       rdf:type owl:SymmetricProperty ,
                                                                owl:TransitiveProperty ;
                                                       rdfs:domain <http://OntoCAPE/upper_level/system.owl#System> ;
                                                       rdfs:range <http://OntoCAPE/upper_level/system.owl#System> ;
                                                       rdfs:comment "The relation isConnectedTo represents topological connectivity between System." ;
                                                       rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#isConsideredUnderAspectOf
<http://OntoCAPE/upper_level/system.owl#isConsideredUnderAspectOf> rdf:type owl:ObjectProperty ;
                                                                   rdfs:range <http://OntoCAPE/upper_level/system.owl#Aspect> ;
                                                                   rdfs:comment "The relation isConsideredUnderAspectOf indicates the type of an AspectSystem by referring to an instance of the Aspect class." ;
                                                                   rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#isDirectSubsystemOf
<http://OntoCAPE/upper_level/system.owl#isDirectSubsystemOf> rdf:type owl:ObjectProperty ;
                                                             rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#isDirectlyRelatedTo> ,
                                                                                <http://OntoCAPE/upper_level/system.owl#isSubsystemOf> ;
                                                             rdfs:comment "The relation isDirectlySubsystemOf links a Subsystem to its direct Supersystem." ;
                                                             rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#isDirectlyConnectedTo
<http://OntoCAPE/upper_level/system.owl#isDirectlyConnectedTo> rdf:type owl:ObjectProperty ;
                                                               rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#isConnectedTo> ,
                                                                                  <http://OntoCAPE/upper_level/system.owl#isDirectlyRelatedTo> ;
                                                               rdf:type owl:SymmetricProperty ;
                                                               rdfs:comment "The relation isDirectlyConnectedTo denotes the direct topological connectedness of two System." ;
                                                               rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#isDirectlyRelatedTo
<http://OntoCAPE/upper_level/system.owl#isDirectlyRelatedTo> rdf:type owl:ObjectProperty ;
                                                             rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#isRelatedTo> ;
                                                             owl:inverseOf <http://OntoCAPE/upper_level/system.owl#isDirectlyRelatedTo> ;
                                                             rdf:type owl:SymmetricProperty ;
                                                             rdfs:comment "The relation isDirectlyRelatedTo subsumes all kinds of direct inter-system relations." ;
                                                             rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#isExclusivelySubsystemOf
<http://OntoCAPE/upper_level/system.owl#isExclusivelySubsystemOf> rdf:type owl:ObjectProperty ;
                                                                  rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#isDirectSubsystemOf> ;
                                                                  rdfs:comment "The relation isExclusivelySubsystemOf links a non-sharable Subsystem to its direct Supersystem." ;
                                                                  rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#isModeledBy
<http://OntoCAPE/upper_level/system.owl#isModeledBy> rdf:type owl:ObjectProperty ;
                                                     rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#isDirectlyRelatedTo> ;
                                                     owl:inverseOf <http://OntoCAPE/upper_level/system.owl#models> ;
                                                     rdfs:domain <http://OntoCAPE/upper_level/system.owl#System> ;
                                                     rdfs:comment "The relation isModeldBy points from a modeled System to its Model." ;
                                                     rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#isObservedAgainstBackdrop
<http://OntoCAPE/upper_level/system.owl#isObservedAgainstBackdrop> rdf:type owl:ObjectProperty ;
                                                                   rdfs:domain <http://OntoCAPE/upper_level/system.owl#Value> ;
                                                                   rdfs:range <http://OntoCAPE/upper_level/system.owl#Value> ;
                                                                   rdfs:comment "The isObservedAgainstBackdrop relation maps a Value against a backdrop Value." ;
                                                                   rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#isPropertyOf
<http://OntoCAPE/upper_level/system.owl#isPropertyOf> rdf:type owl:ObjectProperty ,
                                                               owl:FunctionalProperty ;
                                                      rdfs:domain <http://OntoCAPE/upper_level/system.owl#Property> ;
                                                      rdfs:range <http://OntoCAPE/upper_level/system.owl#System> ;
                                                      rdfs:comment "The relation isPropertyOf links a Property instance to a System instance." ;
                                                      rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#isRelatedTo
<http://OntoCAPE/upper_level/system.owl#isRelatedTo> rdf:type owl:ObjectProperty ;
                                                     owl:inverseOf <http://OntoCAPE/upper_level/system.owl#isRelatedTo> ;
                                                     rdf:type owl:SymmetricProperty ,
                                                              owl:TransitiveProperty ;
                                                     rdfs:domain <http://OntoCAPE/upper_level/system.owl#System> ;
                                                     rdfs:range <http://OntoCAPE/upper_level/system.owl#System> ;
                                                     rdfs:comment "The relation isRelatedTo subsumes all kinds of inter-system relations." ;
                                                     rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#isSubsystemOf
<http://OntoCAPE/upper_level/system.owl#isSubsystemOf> rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#isRelatedTo> ;
                                                       rdf:type owl:TransitiveProperty ;
                                                       rdfs:comment "The relation isSubsystemOf refers from a Subsystem to its Supersystem." ;
                                                       rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#isValueOf
<http://OntoCAPE/upper_level/system.owl#isValueOf> rdf:type owl:ObjectProperty ,
                                                            owl:FunctionalProperty ;
                                                   rdfs:domain <http://OntoCAPE/upper_level/system.owl#Value> ;
                                                   rdfs:range <http://OntoCAPE/upper_level/system.owl#Property> ;
                                                   rdfs:comment "The relation isValueOf assigns a Value to a Property." ;
                                                   rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#models
<http://OntoCAPE/upper_level/system.owl#models> rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#isDirectlyRelatedTo> ;
                                                rdfs:range <http://OntoCAPE/upper_level/system.owl#System> ;
                                                rdfs:comment "The relation 'models' links a Model to the modeled System." ;
                                                rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#qualitativeValue
<http://OntoCAPE/upper_level/system.owl#qualitativeValue> rdf:type owl:ObjectProperty ;
                                                          rdfs:domain <http://OntoCAPE/upper_level/system.owl#QualitativeValue> ;
                                                          rdfs:range <http://OntoCAPE/upper_level/system.owl#ValueEnumeration> ;
                                                          rdfs:comment "The relation qualitativeValue specifies the actual value of a QualitativeValue." ;
                                                          rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#representsAspectOf
<http://OntoCAPE/upper_level/system.owl#representsAspectOf> rdf:type owl:ObjectProperty ;
                                                            rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#isExclusivelySubsystemOf> ;
                                                            rdfs:comment "The relation representsAspectOf links a AspectSystem to its respective System." ;
                                                            rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


#################################################################
#    Data properties
#################################################################

###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#is_at_numbered_tray
:is_at_numbered_tray rdf:type owl:DatatypeProperty ;
                     rdfs:range xsd:integer ;
                     rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/upper_level/system.owl#numericalValue
<http://OntoCAPE/upper_level/system.owl#numericalValue> rdf:type owl:DatatypeProperty ;
                                                        rdfs:comment "The attribute numericalValue specifies the number part of a QuantitativeValue." ;
                                                        rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#value
<http://OntoCAPE/upper_level/system.owl#value> rdf:type owl:DatatypeProperty ;
                                               rdfs:domain <http://OntoCAPE/upper_level/system.owl#QualitativeValue> ;
                                               rdfs:comment "The Value attribute holds the actual value of a QualitativeValue." ;
                                               rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


#################################################################
#    Classes
#################################################################

###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#IntermediateProduct
:IntermediateProduct rdf:type owl:Class ;
