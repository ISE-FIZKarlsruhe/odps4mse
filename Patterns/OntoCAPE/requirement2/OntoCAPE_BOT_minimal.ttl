@prefix : <http://OntoCAPE/chemical_process_system/CPS_function/process.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://OntoCAPE/chemical_process_system/CPS_function/process.owl> .

<http://OntoCAPE/chemical_process_system/CPS_function/process.owl> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2000/01/rdf-schema#comment
rdfs:comment rdfs:isDefinedBy <http://www.w3.org/2000/01/rdf-schema.owl> .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2000/01/rdf-schema#Literal
rdfs:Literal rdfs:isDefinedBy <http://www.w3.org/2000/01/rdf-schema.owl> .


###  http://www.w3.org/2001/XMLSchema#string
xsd:string rdfs:isDefinedBy <http://www.w3.org/2001/xmlschema.owl> .


#################################################################
#    Object Properties
#################################################################

###  http://OntoCAPE/upper_level/network_system.owl#enters
<http://OntoCAPE/upper_level/network_system.owl#enters> rdf:type owl:ObjectProperty ;
                                                        rdfs:subPropertyOf <http://OntoCAPE/upper_level/network_system.owl#isPredecessorOf> ;
                                                        owl:inverseOf <http://OntoCAPE/upper_level/network_system.owl#hasInput> ;
                                                        rdfs:comment "The relation Enters interconnects an ingoing DirectedConnection  to its target Device." ;
                                                        rdfs:isDefinedBy <http://ontocape/upper_level/network_system.owl> .


###  http://OntoCAPE/upper_level/network_system.owl#hasInput
<http://OntoCAPE/upper_level/network_system.owl#hasInput> rdf:type owl:ObjectProperty ;
                                                          rdfs:subPropertyOf <http://OntoCAPE/upper_level/network_system.owl#isSuccessorOf> ;
                                                          rdfs:comment "The relation hasInput connects a Device to an incoming DirectedConnection." ;
                                                          rdfs:isDefinedBy <http://ontocape/upper_level/network_system.owl> .


###  http://OntoCAPE/upper_level/network_system.owl#hasOutput
<http://OntoCAPE/upper_level/network_system.owl#hasOutput> rdf:type owl:ObjectProperty ;
                                                           rdfs:subPropertyOf <http://OntoCAPE/upper_level/network_system.owl#isPredecessorOf> ;
                                                           owl:inverseOf <http://OntoCAPE/upper_level/network_system.owl#leaves> ;
                                                           rdfs:comment "The relation hasOutput connects a Device to an outgoing DirectedConnection." ;
                                                           rdfs:isDefinedBy <http://ontocape/upper_level/network_system.owl> .


###  http://OntoCAPE/upper_level/network_system.owl#isPredecessorOf
<http://OntoCAPE/upper_level/network_system.owl#isPredecessorOf> rdf:type owl:ObjectProperty ;
                                                                 rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#isConnectedTo> ;
                                                                 owl:inverseOf <http://OntoCAPE/upper_level/network_system.owl#isSuccessorOf> ;
                                                                 rdf:type owl:TransitiveProperty ;
                                                                 rdfs:comment "The relation isPredecessorOf identifies all Devices and DirectedConnections that are predecessors of the considered one." ;
                                                                 rdfs:isDefinedBy <http://ontocape/upper_level/network_system.owl> .


###  http://OntoCAPE/upper_level/network_system.owl#isSuccessorOf
<http://OntoCAPE/upper_level/network_system.owl#isSuccessorOf> rdf:type owl:ObjectProperty ;
                                                               rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#isConnectedTo> ;
                                                               rdf:type owl:TransitiveProperty ;
                                                               rdfs:comment "The relation isSuccessorOf identifies all Devices and DirectedConnections that are successors of the considered one." ;
                                                               rdfs:isDefinedBy <http://ontocape/upper_level/network_system.owl> .


###  http://OntoCAPE/upper_level/network_system.owl#leaves
<http://OntoCAPE/upper_level/network_system.owl#leaves> rdf:type owl:ObjectProperty ;
                                                        rdfs:subPropertyOf <http://OntoCAPE/upper_level/network_system.owl#isSuccessorOf> ;
                                                        rdfs:comment "The relation Leaves connects an outgoing DirectedConnection to its source Device." ;
                                                        rdfs:isDefinedBy <http://ontocape/upper_level/network_system.owl> .


###  http://OntoCAPE/upper_level/system.owl#hasDimension
<http://OntoCAPE/upper_level/system.owl#hasDimension> rdf:type owl:ObjectProperty ,
                                                               owl:FunctionalProperty ;
                                                      rdfs:domain [ rdf:type owl:Class ;
                                                                    owl:unionOf ( <http://OntoCAPE/upper_level/system.owl#Property>
                                                                                  <http://OntoCAPE/upper_level/system.owl#UnitOfMeasure>
                                                                                )
                                                                  ] ;
                                                      rdfs:range <http://OntoCAPE/upper_level/system.owl#PhysicalDimension> ;
                                                      rdfs:comment "The relation hasDimension specifies the PhysicalDimension of a PhysicalQuantity or a UnitOfMeasure." ;
                                                      rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#hasProperty
<http://OntoCAPE/upper_level/system.owl#hasProperty> rdf:type owl:ObjectProperty ;
                                                     rdfs:subPropertyOf <http://meta_model/fundamental_concepts/fundamental_concepts.owl#inter-objectRelation> ;
                                                     owl:inverseOf <http://OntoCAPE/upper_level/system.owl#isPropertyOf> ;
                                                     rdf:type owl:InverseFunctionalProperty ;
                                                     rdfs:domain <http://OntoCAPE/upper_level/system.owl#System> ;
                                                     rdfs:range <http://OntoCAPE/upper_level/system.owl#Property> ;
                                                     rdfs:comment "The relation hasProperty indicates the Properties of a System." ;
                                                     rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#hasUnitOfMeasure
<http://OntoCAPE/upper_level/system.owl#hasUnitOfMeasure> rdf:type owl:ObjectProperty ;
                                                          rdfs:subPropertyOf <http://meta_model/fundamental_concepts/fundamental_concepts.owl#object-featureRelation> ;
                                                          rdf:type owl:FunctionalProperty ;
                                                          rdfs:domain <http://OntoCAPE/upper_level/system.owl#QuantitativeValue> ;
                                                          rdfs:range <http://OntoCAPE/upper_level/system.owl#UnitOfMeasure> ;
                                                          rdfs:comment "The relation hasUnitOfMeasure establishes the UnitOfMeasure of a ScalarValue." ;
                                                          rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#hasValue
<http://OntoCAPE/upper_level/system.owl#hasValue> rdf:type owl:ObjectProperty ;
                                                  rdfs:subPropertyOf <http://meta_model/fundamental_concepts/fundamental_concepts.owl#inter-objectRelation> ;
                                                  owl:inverseOf <http://OntoCAPE/upper_level/system.owl#isValueOf> ;
                                                  rdf:type owl:InverseFunctionalProperty ;
                                                  rdfs:domain <http://OntoCAPE/upper_level/system.owl#Property> ;
                                                  rdfs:range <http://OntoCAPE/upper_level/system.owl#Value> ;
                                                  rdfs:comment "The hasValue relation designates the Values of a Property." ;
                                                  rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#isConnectedTo
<http://OntoCAPE/upper_level/system.owl#isConnectedTo> rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf <http://OntoCAPE/upper_level/system.owl#isRelatedTo> ;
                                                       owl:inverseOf <http://OntoCAPE/upper_level/system.owl#isConnectedTo> ;
                                                       rdf:type owl:SymmetricProperty ,
                                                                owl:TransitiveProperty ;
                                                       rdfs:domain <http://OntoCAPE/upper_level/system.owl#System> ;
                                                       rdfs:range <http://OntoCAPE/upper_level/system.owl#System> ;
                                                       rdfs:comment "The relation isConnectedTo represents topological connectivity between System." ;
                                                       rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#isPropertyOf
<http://OntoCAPE/upper_level/system.owl#isPropertyOf> rdf:type owl:ObjectProperty ;
                                                      rdfs:subPropertyOf <http://meta_model/fundamental_concepts/fundamental_concepts.owl#inter-objectRelation> ;
                                                      rdf:type owl:FunctionalProperty ;
                                                      rdfs:domain <http://OntoCAPE/upper_level/system.owl#Property> ;
                                                      rdfs:range <http://OntoCAPE/upper_level/system.owl#System> ;
                                                      rdfs:comment "The relation isPropertyOf links a Property instance to a System instance." ;
                                                      rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#isRelatedTo
<http://OntoCAPE/upper_level/system.owl#isRelatedTo> rdf:type owl:ObjectProperty ;
                                                     rdfs:subPropertyOf <http://meta_model/fundamental_concepts/fundamental_concepts.owl#inter-objectRelation> ;
                                                     owl:inverseOf <http://OntoCAPE/upper_level/system.owl#isRelatedTo> ;
                                                     rdf:type owl:SymmetricProperty ,
                                                              owl:TransitiveProperty ;
                                                     rdfs:domain <http://OntoCAPE/upper_level/system.owl#System> ;
                                                     rdfs:range <http://OntoCAPE/upper_level/system.owl#System> ;
                                                     rdfs:comment "The relation isRelatedTo subsumes all kinds of inter-system relations." ;
                                                     rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#isValueOf
<http://OntoCAPE/upper_level/system.owl#isValueOf> rdf:type owl:ObjectProperty ;
                                                   rdfs:subPropertyOf <http://meta_model/fundamental_concepts/fundamental_concepts.owl#inter-objectRelation> ;
                                                   rdf:type owl:FunctionalProperty ;
                                                   rdfs:domain <http://OntoCAPE/upper_level/system.owl#Value> ;
                                                   rdfs:range <http://OntoCAPE/upper_level/system.owl#Property> ;
                                                   rdfs:comment "The relation isValueOf assigns a Value to a Property." ;
                                                   rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#qualitativeValue
<http://OntoCAPE/upper_level/system.owl#qualitativeValue> rdf:type owl:ObjectProperty ;
                                                          rdfs:subPropertyOf <http://meta_model/fundamental_concepts/fundamental_concepts.owl#object-featureRelation> ;
                                                          rdfs:domain <http://OntoCAPE/upper_level/system.owl#QualitativeValue> ;
                                                          rdfs:range <http://OntoCAPE/upper_level/system.owl#ValueEnumeration> ;
                                                          rdfs:comment "The relation qualitativeValue specifies the actual value of a QualitativeValue." ;
                                                          rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://meta_model/fundamental_concepts/fundamental_concepts.owl#inter-objectRelation
<http://meta_model/fundamental_concepts/fundamental_concepts.owl#inter-objectRelation> rdf:type owl:ObjectProperty ;
                                                                                       rdfs:isDefinedBy <http://meta_model/fundamental_concepts/fundamental_concepts.owl> .


###  http://meta_model/fundamental_concepts/fundamental_concepts.owl#object-featureRelation
<http://meta_model/fundamental_concepts/fundamental_concepts.owl#object-featureRelation> rdf:type owl:ObjectProperty ;
                                                                                         rdfs:isDefinedBy <http://meta_model/fundamental_concepts/fundamental_concepts.owl> .


#################################################################
#    Data properties
#################################################################

###  http://OntoCAPE/upper_level/system.owl#value
<http://OntoCAPE/upper_level/system.owl#value> rdf:type owl:DatatypeProperty ;
                                               rdfs:domain <http://OntoCAPE/upper_level/system.owl#QualitativeValue> ;
                                               rdfs:comment "The Value attribute holds the actual value of a QualitativeValue." ;
                                               rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


#################################################################
#    Classes
#################################################################

###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#IntermediateProduct
:IntermediateProduct rdf:type owl:Class ;
                     owl:disjointWith :RawMaterial ;
                     rdfs:comment "An IntermediateProduct is a ChemicalProduct that is produced in a ProcessStep and is used in another ProcessStep in the whole processing system." ;
                     rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ModeOfOperation
:ModeOfOperation rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Class ;
                                       owl:oneOf ( :Batch
                                                   :Continuous
                                                 )
                                     ] ;
                 rdfs:subClassOf <http://OntoCAPE/meta_model/fundamental_concepts/fundamental_concepts.owl#ValueSet> ;
                 rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ProcessStep
:ProcessStep rdf:type owl:Class ;
             rdfs:subClassOf <http://OntoCAPE/upper_level/system.owl#System> ;
             rdfs:comment "A ProcessStep is a certain material processing procedure." ;
             rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#RawMaterial
:RawMaterial rdf:type owl:Class ;
             rdfs:subClassOf <http://OntoCAPE/upper_level/system.owl#System> ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://OntoCAPE/upper_level/network_system.owl#enters> ;
                               owl:someValuesFrom :ProcessStep
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://OntoCAPE/upper_level/network_system.owl#enters> ;
                               owl:allValuesFrom :ProcessStep
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://OntoCAPE/upper_level/network_system.owl#leaves> ;
                               owl:allValuesFrom :ProcessStep
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://OntoCAPE/upper_level/network_system.owl#leaves> ;
                               owl:cardinality "0"^^xsd:nonNegativeInteger
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://OntoCAPE/upper_level/network_system.owl#enters> ;
                               owl:maxCardinality "1"^^xsd:nonNegativeInteger
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://OntoCAPE/upper_level/network_system.owl#leaves> ;
                               owl:maxCardinality "1"^^xsd:nonNegativeInteger
                             ] ;
             rdfs:comment "A RawMaterial is a Material that enters a ProcessStep." ;
             rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/meta_model/fundamental_concepts/fundamental_concepts.owl#ValueSet
<http://OntoCAPE/meta_model/fundamental_concepts/fundamental_concepts.owl#ValueSet> rdf:type owl:Class ;
                                                                                    rdfs:isDefinedBy <http://ontocape/meta_model/fundamental_concepts/fundamental_concepts.owl> .


###  http://OntoCAPE/upper_level/system.owl#ConstantProperty
<http://OntoCAPE/upper_level/system.owl#ConstantProperty> rdf:type owl:Class ;
                                                          owl:equivalentClass [ owl:intersectionOf ( <http://OntoCAPE/upper_level/system.owl#Property>
                                                                                                     [ rdf:type owl:Restriction ;
                                                                                                       owl:onProperty <http://OntoCAPE/upper_level/system.owl#hasValue> ;
                                                                                                       owl:cardinality "1"^^xsd:nonNegativeInteger
                                                                                                     ]
                                                                                                   ) ;
                                                                                rdf:type owl:Class
                                                                              ] ;
                                                          rdfs:comment "A ConstantProperty is a Property that has exactly one Value." ;
                                                          rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#ExtensibleValueSet
<http://OntoCAPE/upper_level/system.owl#ExtensibleValueSet> rdf:type owl:Class ;
                                                            rdfs:subClassOf <http://OntoCAPE/upper_level/system.owl#ValueEnumeration> ;
                                                            owl:disjointWith <http://OntoCAPE/upper_level/system.owl#FixedValueSet> ;
                                                            rdfs:comment "An ExtensibleValueSet is a ValueEnumeration which, unlike a FixedValueSet, is not defined by an (exhaustive) enumeration of its instances. Thus, the number of possible values may change at run time." ;
                                                            rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#FixedValueSet
<http://OntoCAPE/upper_level/system.owl#FixedValueSet> rdf:type owl:Class ;
                                                       rdfs:subClassOf <http://OntoCAPE/upper_level/system.owl#ValueEnumeration> ;
                                                       rdfs:comment "A FixedValueSet is a ValueEnumeration that is defined by an exhaustive enumeration of its instances. Thus, the number of possible values is fixed." ;
                                                       rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#PhysicalDimension
<http://OntoCAPE/upper_level/system.owl#PhysicalDimension> rdf:type owl:Class ;
                                                           rdfs:subClassOf <http://meta_model/fundamental_concepts/fundamental_concepts.owl#NonExhaustiveValueSet> ;
                                                           owl:disjointWith <http://OntoCAPE/upper_level/system.owl#UnitOfMeasure> ;
                                                           rdfs:comment "A PhysicalDimension is a characteristic associated with PhysicalQuantities and UnitsOfMeasure for purposes of organization or differentiation. Mass, length, and force are exemplary instances of PhysicalDimension." ;
                                                           rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#PhysicalQuantity
<http://OntoCAPE/upper_level/system.owl#PhysicalQuantity> rdf:type owl:Class ;
                                                          owl:equivalentClass [ owl:intersectionOf ( <http://OntoCAPE/upper_level/system.owl#Property>
                                                                                                     [ rdf:type owl:Restriction ;
                                                                                                       owl:onProperty <http://OntoCAPE/upper_level/system.owl#hasDimension> ;
                                                                                                       owl:someValuesFrom <http://OntoCAPE/upper_level/system.owl#PhysicalDimension>
                                                                                                     ]
                                                                                                   ) ;
                                                                                rdf:type owl:Class
                                                                              ] ;
                                                          rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                            owl:onProperty <http://OntoCAPE/upper_level/system.owl#hasDimension> ;
                                                                            owl:allValuesFrom <http://OntoCAPE/upper_level/system.owl#PhysicalDimension>
                                                                          ] ,
                                                                          [ rdf:type owl:Restriction ;
                                                                            owl:onProperty <http://OntoCAPE/upper_level/system.owl#hasValue> ;
                                                                            owl:allValuesFrom <http://OntoCAPE/upper_level/system.owl#QuantitativeValue>
                                                                          ] ,
                                                                          [ rdf:type owl:Restriction ;
                                                                            owl:onProperty <http://OntoCAPE/upper_level/system.owl#hasDimension> ;
                                                                            owl:cardinality "1"^^xsd:nonNegativeInteger
                                                                          ] ;
                                                          rdfs:comment "A PhysicalQuantity is a Property that has quantifiable Values. The concept includes scalars as well as vectors and higher-order tensors. Moreover, it comprises both physical quantities, such as mass or velocity, and nonphysical quantities, such as amount of money or rate of inflation" ;
                                                          rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#Property
<http://OntoCAPE/upper_level/system.owl#Property> rdf:type owl:Class ;
                                                  rdfs:subClassOf <http://meta_model/fundamental_concepts/fundamental_concepts.owl#Object> ,
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty <http://OntoCAPE/upper_level/system.owl#hasValue> ;
                                                                    owl:allValuesFrom <http://OntoCAPE/upper_level/system.owl#Value>
                                                                  ] ,
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty <http://OntoCAPE/upper_level/system.owl#isPropertyOf> ;
                                                                    owl:allValuesFrom <http://OntoCAPE/upper_level/system.owl#System>
                                                                  ] ,
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty <http://OntoCAPE/upper_level/system.owl#isPropertyOf> ;
                                                                    owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                                                  ] ;
                                                  owl:disjointWith <http://OntoCAPE/upper_level/system.owl#System> ,
                                                                   <http://OntoCAPE/upper_level/system.owl#Value> ;
                                                  rdfs:comment "The Property class represents the individual properties (traits, qualities) of a System, which distinguish the System from others. Typical examples are Size, Color, or Weight, which are modeled as subclasses of Property." ;
                                                  rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#QualitativeValue
<http://OntoCAPE/upper_level/system.owl#QualitativeValue> rdf:type owl:Class ;
                                                          rdfs:subClassOf <http://OntoCAPE/upper_level/system.owl#Value> ,
                                                                          [ rdf:type owl:Class ;
                                                                            owl:unionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                                                   owl:onProperty <http://OntoCAPE/upper_level/system.owl#qualitativeValue> ;
                                                                                                                   owl:cardinality "0"^^xsd:nonNegativeInteger
                                                                                                                 ]
                                                                                                                 [ rdf:type owl:Restriction ;
                                                                                                                   owl:onProperty <http://OntoCAPE/upper_level/system.owl#value> ;
                                                                                                                   owl:cardinality "1"^^xsd:nonNegativeInteger
                                                                                                                 ]
                                                                                                               ) ;
                                                                                            rdf:type owl:Class
                                                                                          ]
                                                                                          [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                                                   owl:onProperty <http://OntoCAPE/upper_level/system.owl#qualitativeValue> ;
                                                                                                                   owl:cardinality "1"^^xsd:nonNegativeInteger
                                                                                                                 ]
                                                                                                                 [ rdf:type owl:Restriction ;
                                                                                                                   owl:onProperty <http://OntoCAPE/upper_level/system.owl#value> ;
                                                                                                                   owl:cardinality "0"^^xsd:nonNegativeInteger
                                                                                                                 ]
                                                                                                               ) ;
                                                                                            rdf:type owl:Class
                                                                                          ]
                                                                                        )
                                                                          ] ,
                                                                          [ rdf:type owl:Class ;
                                                                            owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                            owl:onProperty <http://OntoCAPE/upper_level/system.owl#qualitativeValue> ;
                                                                                            owl:someValuesFrom <http://OntoCAPE/upper_level/system.owl#ValueEnumeration>
                                                                                          ]
                                                                                          [ rdf:type owl:Restriction ;
                                                                                            owl:onProperty <http://OntoCAPE/upper_level/system.owl#value> ;
                                                                                            owl:cardinality "1"^^xsd:nonNegativeInteger
                                                                                          ]
                                                                                        )
                                                                          ] ,
                                                                          [ rdf:type owl:Restriction ;
                                                                            owl:onProperty <http://OntoCAPE/upper_level/system.owl#qualitativeValue> ;
                                                                            owl:allValuesFrom <http://OntoCAPE/upper_level/system.owl#ValueEnumeration>
                                                                          ] ;
                                                          owl:disjointWith <http://OntoCAPE/upper_level/system.owl#QuantitativeValue> ;
                                                          rdfs:comment "A QualitativeValue is a value that is not (numerically) quantifiable." ;
                                                          rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#QuantitativeValue
<http://OntoCAPE/upper_level/system.owl#QuantitativeValue> rdf:type owl:Class ;
                                                           owl:equivalentClass [ owl:intersectionOf ( <http://OntoCAPE/upper_level/system.owl#Value>
                                                                                                      [ rdf:type owl:Restriction ;
                                                                                                        owl:onProperty <http://OntoCAPE/upper_level/system.owl#isValueOf> ;
                                                                                                        owl:someValuesFrom <http://OntoCAPE/upper_level/system.owl#PhysicalQuantity>
                                                                                                      ]
                                                                                                    ) ;
                                                                                 rdf:type owl:Class
                                                                               ] ;
                                                           rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                             owl:onProperty <http://OntoCAPE/upper_level/system.owl#isValueOf> ;
                                                                             owl:allValuesFrom <http://OntoCAPE/upper_level/system.owl#PhysicalQuantity>
                                                                           ] ;
                                                           rdfs:comment "A QuantitativeValue is the value of a PhysicalQuantity." ;
                                                           rdfs:isDefinedBy <http://ontocape/upper_level/system.owl> .


###  http://OntoCAPE/upper_level/system.owl#System
<http://OntoCAPE/upper_level/system.owl#System> rdf:type owl:Class ;
                                                rdfs:subClassOf <http://meta_model/fundamental_concepts/fundamental_concepts.owl#Object> ,
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty <http://OntoCAPE/upper_level/system.owl#hasProperty> ;
                                                                  owl:allValuesFrom <http://OntoCAPE/upper_level/system.owl#Property>
