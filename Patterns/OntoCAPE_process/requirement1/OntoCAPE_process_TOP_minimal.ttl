###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#AggregatedProcessStep
:AggregatedProcessStep rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( :ProcessStep
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty <http://OntoCAPE/upper_level/system.owl#hasDirectSubsystem> ;
                                                                    owl:someValuesFrom :ProcessStep
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:comment "An AggregatedProcessStep is a ProcessStep which consists of a number of ProcessSteps in a certain sequence." ;
                       rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#BatchProcess
:BatchProcess rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( :ProcessState
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :hasOperationMode ;
                                                           owl:hasValue :Batch
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              owl:disjointWith :ProcessStream ;
              rdfs:comment "A BatchProcess indicates the batch mode of operation applied to the Process." ;
              rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#By-product
:By-product rdf:type owl:Class ;
            rdfs:subClassOf :EndProduct ;
            owl:disjointWith :Co-product ,
                             :CoreProduct ;
            rdfs:comment "A Byproduct is an EndProduct whose production is unavoidable while a CoreProduct is produced." ;
            rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#Co-product
:Co-product rdf:type owl:Class ;
            rdfs:subClassOf :EndProduct ;
            owl:disjointWith :CoreProduct ;
            rdfs:comment "A Co- product is an EndProduct whose production is unintended." ;
            rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#CoreProduct
:CoreProduct rdf:type owl:Class ;
             rdfs:subClassOf :EndProduct ;
             rdfs:comment "A CoreProduct is a main (or an intended) EndProduct." ;
             rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#EndProduct
:EndProduct rdf:type owl:Class ;
            rdfs:subClassOf :OutputProduct ;
            owl:disjointWith :WasteProduct ;
            rdfs:comment "An EndProduct is an OutputProduct that is valuable." ;
            rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#EnthalpyChange
:EnthalpyChange rdf:type owl:Class ;
                rdfs:subClassOf :UnitOperation ;
                owl:disjointWith :Separation ;
                rdfs:comment "EnthalpyChange in terms of unit operation considers a conversion of energy which often result in a change of the state of aggregation." ;
                rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#FlashVaporRatio
:FlashVaporRatio rdf:type owl:Class ;
                 owl:disjointWith :KeyChemicalComponentConversionRatio ;
                 rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#Flashing
:Flashing rdf:type owl:Class ;
          rdfs:subClassOf :Separation ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasVapourRatio ;
                            owl:someValuesFrom :FlashVaporRatio
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasVapourRatio ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger
                          ] ;
          owl:disjointWith :Leaching ;
          rdfs:comment "Flashing utilized the state of phase equilibrium between a vapor and a liquid phase and the resulting material transport if a mixture with different fugacity is existent. In terms of unit operation this is usually accomplished in a vessel." ;
          rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#IntermediateProduct
:IntermediateProduct rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( :ProcessState
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty <http://OntoCAPE/upper_level/network_system.owl#enters> ;
                                                                  owl:someValuesFrom :ProcessStep
                                                                ]
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty <http://OntoCAPE/upper_level/network_system.owl#leaves> ;
                                                                  owl:someValuesFrom :ProcessStep
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     owl:disjointWith :RawMaterial ;
                     rdfs:comment "An IntermediateProduct is a ChemicalProduct that is produced in a ProcessStep and is used in another ProcessStep in the whole processing system." ;
                     rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#KeyChemicalComponentConversionRatio
:KeyChemicalComponentConversionRatio rdf:type owl:Class ;
                                     rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#Leaching
:Leaching rdf:type owl:Class ;
          rdfs:subClassOf :Separation ;
          rdfs:comment "Leaching in terms of unit operation is a particular form of extraction. Usually, a solute, a insolvable carrier and a solvent are applied for solid extraction" ;
          rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#Mixing
:Mixing rdf:type owl:Class ;
        rdfs:subClassOf :UnitOperation ;
        rdfs:comment "Mixing in terms of unit operations is a special type of combination which results in a mixture which is required for further processing. It is usually accomplished by means of stirrer." ;
        rdfs:isDefinedBy <http://ontocape/chemical_process_system/cps_function/process.owl> .


###  http://OntoCAPE/chemical_process_system/CPS_function/process.owl#ModeOfOperation
:ModeOfOperation rdf:type owl:Class ;
